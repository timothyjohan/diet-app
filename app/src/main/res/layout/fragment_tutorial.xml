<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".menu.Tutorial">

    <ImageView
        android:id="@+id/ivSetting"
        android:layout_width="118dp"
        android:layout_height="182dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="21dp"
        android:src="@drawable/screenshot_2024_06_21_040227"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPenjelasan3"
        app:srcCompat="@drawable/screenshot_2024_06_21_040332" />

    <TextView
        android:id="@+id/tvSetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="79dp"
        android:text="Settings"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvRecap"
        app:layout_constraintTop_toBottomOf="@+id/ivLogFood" />

    <TextView
        android:id="@+id/tvPenjelasan4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="10dp"
        android:text="Setting difungsikan \nuntuk mengatur konfigurasi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvRecap" />

    <TextView
        android:id="@+id/tvTutorial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="165dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="165dp"
        android:text="Tutorial"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvCalculator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="90dp"
        android:text="TDEE Calculator"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvPenjelasan1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="12dp"
        android:text="TDEE Calculator"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvCalculator" />

    <ImageView
        android:id="@+id/ivCalculator"
        android:layout_width="118dp"
        android:layout_height="182dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/screenshot_2024_06_21_040227"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPenjelasan1" />

    <ImageView
        android:id="@+id/ivRecap"
        android:layout_width="118dp"
        android:layout_height="182dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/screenshot_2024_06_21_040227"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPenjelasan3"
        app:srcCompat="@drawable/screenshot_2024_06_21_040318" />

    <TextView
        android:id="@+id/tvLogFood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="73dp"
        android:text="Log Food"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvPenjelasan2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="82dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvPenjelasan1"
        app:layout_constraintTop_toBottomOf="@+id/tvCalculator" />

    <ImageView
        android:id="@+id/ivLogFood"
        android:layout_width="118dp"
        android:layout_height="182dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="21dp"
        android:src="@drawable/screenshot_2024_06_21_040227"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivCalculator"
        app:layout_constraintTop_toBottomOf="@+id/tvPenjelasan2"
        app:srcCompat="@drawable/screenshot_2024_06_21_040300" />

    <TextView
        android:id="@+id/tvRecap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="24dp"
        android:text="Recap"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivCalculator" />

    <TextView
        android:id="@+id/tvPenjelasan3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="12dp"
        android:text="Recap difungsikan untuk \nmelihat kesuksesan\n diet setiap harinya"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvRecap" />
</androidx.constraintlayout.widget.ConstraintLayout>